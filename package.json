{
  "name": "ibm-garage-cli-tools",
  "version": "0.0.15",
  "org": "garagecatalyst",
  "description": "This repository contains a docker image to help setup an IBM Cloud Public development environment.",
  "main": "index.js",
  "scripts": {
    "build": "./scripts/build.sh $npm_package_name $npm_package_version",
    "pretest": "npm run build",
    "test": "npm run test:only",
    "test:only": "./scripts/test.sh $npm_package_name $npm_package_version",
    "prepush": "npm test",
    "push": "npm run push:only",
    "push:only": "./scripts/publish.sh $npm_package_org $npm_package_name $npm_package_version",
    "pull": "./scripts/pull.sh $npm_package_org $npm_package_name $npm_package_version",
    "start": "docker run -it $npm_package_name:$npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:garage-catalyst/client-tools-image.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
